<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.caiwei.customui"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:launchMode="singleTask">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".LayoutActivity"
            android:launchMode="singleTask"/>
        <activity android:name=".XferModeActivity">
        </activity>
        <activity android:name=".MaskFilterActivity">
        </activity>
        <activity android:name=".CanvasActivity">
        </activity>
        <activity android:name=".RevealActivity">
        </activity>
        <activity android:name=".SearchViewActivity">
        </activity>
        <activity android:name=".PaintActivity">
        </activity>
        <activity android:name=".PathActivity">
        </activity>
    </application>

</manifest>